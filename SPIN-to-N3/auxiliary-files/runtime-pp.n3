# CHAIN PREDICATES
# - optional chain
#
{
   ( ?a ?a ) :optionalChain ?predicate .
}
<= {} .

#
{
   ( ?a ?b ) :optionalChain ?predicate .
}
<= 
{
   ( ?a ?b ) :chain ?predicate .
} .


# - chain
#
{
   ( ?a ?b ) :chain ?predicate .
}
<= 
{
   ?a ?predicate ?b .
} .

#
{
   ( ?a ?x ) :chain ?predicate .
}
<= 
{
   ?a ?predicate ?b .
   ( ?b ?x ) :chain ?predicate 
} .


# - optional
#
{
   ( ?a ?a ) :optional ?predicate 
}
<= {} .

#
{
   ( ?a ?b ) :optional ?predicate 
}
<= 
{
   ?a ?predicate ?b 
} .
