                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="https://contsys.org/concept/care_period_mandate"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">demand for healthcare provider activities expressed by a healthcare actor</rdfs:comment>
        <rdfs:label xml:lang="en">demandForCare</rdfs:label>
        <rdfs:label xml:lang="es">demanda de atencin</rdfs:label>
    </owl:Class>
    


    <!-- https://contsys.org/concept/demand_for_initial_contact -->

