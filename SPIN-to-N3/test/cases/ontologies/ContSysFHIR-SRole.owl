                <owl:onClass rdf:resource="https://contsys.org/concept/health_objective"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">desired achievement of one or more healthcare activities, considered as an intermediate operational step to reach a specific health objective</rdfs:comment>
        <rdfs:label xml:lang="en">careGoal</rdfs:label>
        <skos:note xml:lang="en">NOTE A healthcare goal could be expressed as one or several target conditions to be reached within a specified date and time.</skos:note>
    </owl:Class>
    


