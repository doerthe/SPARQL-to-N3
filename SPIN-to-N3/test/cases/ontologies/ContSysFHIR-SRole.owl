                <owl:someValuesFrom rdf:resource="https://contsys.org/concept/health_record"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#isPlannedIn"/>
                <owl:someValuesFrom rdf:resource="https://contsys.org/concept/care_plan"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/for-coc#isPerformedFor"/>
