{
   ( "abcdec" "c" ) :strafter ?result 
}
=> 
{
   :success3 :after ?result 
} .

# (empty string)
{
   ( "abcdec" "q" ) :strafter ?result 
}
=> 
{
   :success4 :after ?result 
} .

#
{
   ( "abcdec"@en "" ) :strafter ?result 
}
=> 
{
   :success5 :after ?result 
} .

# (incompatible language tags)
{
   ( "abcdec" ""@de ) :strafter ?result 
}
=> 
{
   :error2 :after ?result 
} .
