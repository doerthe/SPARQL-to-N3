<?xml version="1.0"?>

<tells
  xmlns="http://dl.kr.org/dig/lang"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://dl.kr.org/dig/lang http://potato.cs.man.ac.uk/dig/level0/dig.xsd">
  <defindividual name="http://example.org/foo#a0" />
  <defindividual name="http://example.org/foo#a1" />
  <defindividual name="http://example.org/foo#a2" />
  <defconcept name="http://example.org/foo#A" />

  <disjoint>
  	<iset>
  		<individual name="http://example.org/foo#a1" />
  	</iset>
  	<iset>
  		<individual name="http://example.org/foo#a0" />
  	</iset>
  </disjoint>
  
  <disjoint>
  	<iset>
  		<individual name="http://example.org/foo#a1" />
  	</iset>
  	<iset>
  		<individual name="http://example.org/foo#a2" />
  	</iset>
  </disjoint>
  
  <disjoint>
  	<iset>
  		<individual name="http://example.org/foo#a2" />
  	</iset>
  	<iset>
  		<individual name="http://example.org/foo#a0" />
  	</iset>
  </disjoint>
  
  <instanceof>
  	<individual name="http://example.org/foo#a0" />
  	<catom name="http://example.org/foo#A" />
  </instanceof>
  
  <instanceof>
  	<individual name="http://example.org/foo#a1" />
  	<catom name="http://example.org/foo#A" />
  </instanceof>
  
  <instanceof>
  	<individual name="http://example.org/foo#a2" />
  	<catom name="http://example.org/foo#A" />
  </instanceof>
  
</tells>
