<?xml version="1.0"?>

<tells
  xmlns="http://dl.kr.org/dig/lang"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://dl.kr.org/dig/lang http://potato.cs.man.ac.uk/dig/level0/dig.xsd">
  <defconcept name="http://example.org/foo#A" />
  <defconcept name="http://example.org/foo#B" />
  <defconcept name="anon:" />
  <defindividual name="http://example.org/foo#b0" />
  <defindividual name="http://example.org/foo#b1" />

  <equalc>
  	<catom name="http://example.org/foo#A" />
  	<catom name="anon:" />
  </equalc>
  
  <equalc>
  	<catom name="anon:" />
  	<iset>
  		<individual name="http://example.org/foo#b0" />
  		<individual name="http://example.org/foo#b1" />
  	</iset>
  </equalc>
  
  <instanceof>
    <individual name="http://example.org/foo#b0"/>
    <catom name="http://example.org/foo#B"/>
  </instanceof>

  <instanceof>
    <individual name="http://example.org/foo#b1"/>
    <catom name="http://example.org/foo#B"/>
  </instanceof>

</tells>
