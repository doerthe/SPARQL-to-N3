        rdfs:comment
            "Example 2 from http://lists.w3.org/Archives/Public/public-rdf-dawg/2005JulSep/0096" ;
        mf:action
            [ qt:data   <normalization-03.ttl> ;
              qt:query  <normalization-03.rq> ] ;
        mf:result  <normalization-03-results.ttl> ;
